{
  "schemaVersion": 1,
  "id": "nylon",
  "version": "${version}",
  "name": "Nylon",
  "description": "Library for creating serverside content using Animated-Java for custom models and animations.",
  "authors": [
    "Pinnit",
    "Wesley1808"
  ],
  "contact": {
    "sources": "https://github.com/Provim-Gaming/nylon",
    "issues": "https://github.com/Provim-Gaming/nylon/issues"
  },
  "license": "GPL-3.0",
  "environment": "*",
  "entrypoints": {
    "main": [
      "org.provim.nylon.Nylon"
    ]
  },
  "mixins": [
    "nylon.mixins.json"
  ],
  "depends": {
    "minecraft": ">=1.20.2",
    "fabricloader": "*",
    "fabric-api": "*",
    "polymer-core": "*",
    "polymer-networking": "*",
    "polymer-virtual-entity": "*",
    "polymer-resource-pack": "*"
  }
